{
  "version": 2,
  "builds": [
    {
      "src": "okuafoadamfo-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "cleanUrls": true
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/okuafoadamfo-frontend/index.html"
    }
  ]
}