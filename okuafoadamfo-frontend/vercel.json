{
  "version": 2,
  "builds": [
    {
      "src": "okuafoadamfo-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "cleanUrls": true
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "okuafoadamfo-frontend",
      "headers": {
        "Cache-Control": "s-maxage=0"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/okuafoadamfo-frontend/index.html"
    }
  ]
}